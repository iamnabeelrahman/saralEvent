{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Event_Name": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Date_and_Time": {
      "type": "datetime"
    },
    "Venue": {
      "type": "string"
    },
    "Display_Picture": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "events"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "Event"
    },
    "slug": {
      "type": "uid",
      "targetField": "Event_Name",
      "required": true
    },
    "isFeatured": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "Event_Id": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
